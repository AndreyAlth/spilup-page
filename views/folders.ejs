<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <%- include ('partials/head') %>

    <title>Carpetas - Spilup</title>
    <link rel="stylesheet" href="estilos.css">
  </head>
  <body>
    <!-- header -->
    <%- include ('partials/header') %>
    <!-- header -->

    <!-- carpeta -->
    <div class="container">
        <div class="row">
        <div class="col text-center m-4">
            <h4>Carpetas</h4>
        </div>
        </div>
        <div class="row">
        <div class="col mt-4 mb-4">
            <div class="card" style="width: 18rem;">
            <img src="https://spilup.s3-us-west-1.amazonaws.com/carpetas/c.jpg" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">Carpeta Normal</h5>
                <p class="card-text">Carpeta sencilla de trabajo 1pz. <br>
                <br>$5
                </p>
                <form target="paypal" action="https://www.paypal.com/cgi-bin/webscr" method="post">
                <input type="hidden" name="cmd" value="_s-xclick">
                <input type="hidden" name="hosted_button_id" value="3UCG7VB6JG4LC">
                <input type="image" src="https://www.paypalobjects.com/es_XC/i/btn/btn_cart_LG.gif" border="0" name="submit" alt="PayPal, la forma más segura y rápida de pagar en línea.">
                <img alt="" border="0" src="https://www.paypalobjects.com/es_XC/i/scr/pixel.gif" width="1" height="1">
                </form>
             </div>
            </div>
        </div>
        <div class="col mt-4 mb-4">
            <div class="card" style="width: 18rem;">
            <img src="https://spilup.s3-us-west-1.amazonaws.com/carpetas/folder.jpg" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">Carpeta papel reciclado</h5>
                <p class="card-text">Carpeta presentable para trabajo. <br>
                <br>$3
            </p>
            <form target="paypal" action="https://www.paypal.com/cgi-bin/webscr" method="post">
            <input type="hidden" name="cmd" value="_s-xclick">
            <input type="hidden" name="hosted_button_id" value="2QUYQEMW8BQRU">
            <input type="image" src="https://www.paypalobjects.com/es_XC/i/btn/btn_cart_LG.gif" border="0" name="submit" alt="PayPal, la forma más segura y rápida de pagar en línea.">
            <img alt="" border="0" src="https://www.paypalobjects.com/es_XC/i/scr/pixel.gif" width="1" height="1">
            </form>
             </div>
            </div>
        </div>
        </div>
    </div>
    <!-- carpeta -->
    <!-- footer -->
    <%- include ('partials/footer') %>
    <!-- footer -->
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
  </body>
</html>
